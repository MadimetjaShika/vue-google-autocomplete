{
  "name": "vuetify-google-autocomplete",
  "version": "2.0.0-beta.3",
  "description": "A Vuetify ready Vue.jS component to Google Places Autocomplete",
  "main": "dist/index.js",
  "homepage": "https://github.com/MadimetjaShika/vuetify-google-autocomplete",
  "repository": {
    "type": "git",
    "url": "https://github.com/MadimetjaShika/vuetify-google-autocomplete.git"
  },
  "keywords": [
    "vue",
    "vuetify",
    "google",
    "autocomplete"
  ],
  "scripts": {
    "dev": "npm run build && cross-env NODE_ENV=development webpack --progress --hide-modules",
    "docs": "./node_modules/.bin/jsdoc -c jsdoc.json && node ./update-docs.js",
    "build": "mkdir -p ./dist && babel ./src/ -d ./dist",
    "minify": "minify ./dist/ -d ./dist",
    "watch": "cross-env NODE_ENV=development webpack --watch --progress --hide-modules",
    "lint": "eslint --ext .js,.vue src test/unit/specs",
    "hot": "cross-env NODE_ENV=development webpack-dev-server --inline --hot",
    "production": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "deploy:example": "npm run docs && cp -rf docs example && gh-pages -d example/dist example/docs",
    "test": "NODE_PATH=test ava --verbose"
  },
  "files": [
    "dist"
  ],
  "ava": {
    "require": [
      "babel-register",
      "./test/helpers/setup.js"
    ]
  },
  "author": "Madi Shika <mmjshika@gmail.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-minify": "^0.3.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "browser-env": "^3.2.5",
    "cross-env": "^5.1.3",
    "docdash": "^0.4.0",
    "eslint": "^4.18.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.9.0",
    "fs-path": "0.0.24",
    "gh-pages": "^1.2.0",
    "jsdoc": "^3.5.5",
    "jsdoc-to-markdown": "^4.0.1",
    "laravel-mix": "^0.8.8",
    "vue": "^2.5.16",
    "vue-clipboards": "^1.2.4",
    "vue-node": "^1.1.1",
    "vuetify": "^1.0.19",
    "webpack": "^3.11.0"
  }
}
